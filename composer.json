{
    "name": "kosmosx/auth",
    "description": "Auth service for Laravel / Lumen framework.",
    "keywords": ["framework", "laravel", "lumen", "auth", "jwt", "api"],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Fabrizio Cafolla",
            "email": "developer@fabriziocafolla.com"
        }
    ],

    "require": {
        "php": "^7.1.3",
        "tymon/jwt-auth": "1.0.*@dev",
        "kosmosx/core": "dev-develop"
    },
    "require-dev": {},

    "autoload": {
        "psr-4": {
            "Kosmosx\\Auth\\": "src/"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Kosmosx\\Auth\\AuthServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
